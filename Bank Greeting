def main():
    greeting = input("How would you greet me? ").lower().strip().split()
    first = greeting[0]
    if len(first) < 5:
        if first[0] == "h":
            print("$20")
        else:
            print("100")
    else:
        firstfive = first[0]+first[1]+first[2]+first[3]+first[4]
        if firstfive == "hello":
            print("$0")
        elif first[0] == "h":
            print("$20")
        else:
            print("$100")

main()
